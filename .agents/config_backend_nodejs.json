{
  "agents": {
    "nodejs-developer": {
      "type": "backend-developer",
      "description": "Node.js/Express backend developer",
      "model": "sonnet",
      "file_patterns": [
        "**/*.ts",
        "**/*.js"
      ],
      "tools": [
        "Read",
        "Edit",
        "Write",
        "Bash"
      ],
      "preferences": {
        "formatter": "prettier",
        "linter": "eslint",
        "test_framework": "jest"
      }
    },
    "nodejs-test-specialist": {
      "type": "test-specialist",
      "description": "Node.js test specialist using Jest",
      "model": "sonnet",
      "file_patterns": [
        "**/*.test.ts",
        "**/*.spec.ts"
      ],
      "tools": [
        "Read",
        "Edit",
        "Write",
        "Bash"
      ],
      "preferences": {
        "test_command": "npm test -- --coverage"
      }
    },
    "backend-code-reviewer": {
      "type": "code-reviewer",
      "description": "Backend code reviewer for Node.js security and best practices",
      "model": "sonnet",
      "tools": [
        "Read",
        "Grep"
      ]
    },
    "integration-tester": {
      "type": "integration-tester",
      "description": "Execute tests and enforce quality gates",
      "model": "sonnet",
      "tools": [
        "Bash",
        "Read"
      ],
      "preferences": {
        "test_command": "npm test -- --coverage"
      }
    }
  }
}
