{
  "agents": {
    "angular-developer": {
      "type": "frontend-developer",
      "description": "Angular frontend developer",
      "model": "sonnet",
      "file_patterns": [
        "**/*.ts",
        "**/*.html"
      ],
      "tools": [
        "Read",
        "Edit",
        "Write",
        "Bash"
      ]
    },
    "angular-test-specialist": {
      "type": "test-specialist",
      "description": "Angular test specialist using Jasmine/Karma and Angular TestBed",
      "model": "sonnet",
      "file_patterns": [
        "**/*.spec.ts"
      ],
      "tools": [
        "Read",
        "Edit",
        "Write",
        "Bash"
      ]
    },
    "frontend-code-reviewer": {
      "type": "code-reviewer",
      "description": "Frontend code reviewer for Angular, accessibility, and best practices",
      "model": "sonnet",
      "tools": [
        "Read",
        "Grep"
      ]
    },
    "integration-tester": {
      "type": "integration-tester",
      "description": "Execute tests and enforce quality gates",
      "model": "sonnet",
      "tools": [
        "Bash",
        "Read"
      ],
      "preferences": {
        "test_command": "ng test --watch=false --code-coverage",
        "build_command": "ng build",
        "lint_command": "ng lint"
      }
    }
  }
}
