{
  "agents": {
    "react-frontend-dev": {
      "type": "frontend-developer",
      "description": "React/Next.js frontend developer",
      "model": "sonnet",
      "file_patterns": [
        "**/*.tsx",
        "**/*.jsx",
        "**/*.ts",
        "**/*.js"
      ],
      "tools": [
        "Read",
        "Edit",
        "Write",
        "Bash"
      ],
      "preferences": {
        "formatter": "prettier",
        "linter": "eslint",
        "test_framework": "jest",
        "testing_library": "react-testing-library"
      }
    },
    "react-test-specialist": {
      "type": "test-specialist",
      "description": "React test specialist using Jest and React Testing Library",
      "model": "sonnet",
      "file_patterns": [
        "**/*.test.tsx",
        "**/*.test.ts"
      ],
      "tools": [
        "Read",
        "Edit",
        "Write",
        "Bash"
      ]
    },
    "frontend-code-reviewer": {
      "type": "code-reviewer",
      "description": "Frontend code reviewer for React, accessibility, and best practices",
      "model": "sonnet",
      "tools": [
        "Read",
        "Grep"
      ]
    },
    "integration-tester": {
      "type": "integration-tester",
      "description": "Execute tests and enforce quality gates",
      "model": "sonnet",
      "tools": [
        "Bash",
        "Read"
      ],
      "preferences": {
        "test_command": "npm test -- --coverage"
      }
    }
  }
}
